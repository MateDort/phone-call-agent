# Twilio Configuration (Required)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890
TARGET_PHONE_NUMBER=+1234567890

# Webhook Configuration (Required)
# For production, use your ngrok/public URL: https://your-domain.ngrok.io
# For local testing: http://localhost:5000
WEBHOOK_BASE_URL=http://localhost:5000
WEBHOOK_PORT=5000
WEBSOCKET_PORT=5001

# Gemini Configuration (Required - replaces ElevenLabs and other LLMs)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=models/gemini-2.5-flash-native-audio-preview-12-2025
GEMINI_VOICE=Kore

# Agent Configuration
AUTO_CALL=false  # Set to true to automatically make a call on startup

# Audio Configuration
AUDIO_SAMPLE_RATE=8000  # Twilio uses 8kHz Î¼-law audio

